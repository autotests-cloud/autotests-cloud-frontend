.mockup-code {
  height: 485px;
  max-height: 485px;
  background: #3c4850;
}

.mockup-code {
  margin-top: 35px;
  padding-top: 0.5rem;
  border-radius: 0.5rem;
  border-top-left-radius: 0;
  border-top-right-radius: 0;
}

.mockup-code code {
  text-align: left;
}

.mockup-code pre[data-prefix]:before {
  width: auto;
  content: attr(data-prefix);
  content: attr(data-prefix);
  display: inline-block;
  text-align: right;
  opacity: 0.5;
  margin-right: 1.25rem;
}

.mockup-code::before {
  display: none;
}

.mockup-code__window {
  width: 100%;
  height: 35px;
  background: #607482;
  opacity: 1;
  top: 0;
  left: 0;
  z-index: 10;
  border-top-left-radius: 0.5rem;
  border-top-right-radius: 0.5rem;
}

.mockup-code__window::before {
  content: "";
  border-radius: 9999px;
  display: block;
  height: 0.75rem;
  margin-bottom: 1rem;
  opacity: 0.3;
  width: 0.75rem;
  box-shadow: 1.4em 0, 2.8em 0, 4.2em 0;
  transform: translateX(0px) translateY(13px);
  box-sizing: border-box;
  border-width: 0;
  border-style: solid;
  border-color: #e5e7eb;
}

pre {
  display: flex;
  white-space: pre-wrap;
  /* css-3 */
  white-space: -moz-pre-wrap;
  /* Mozilla*/
  white-space: -pre-wrap;
  /* Opera 4-6 */
  white-space: -o-pre-wrap;
  /* Opera 7 */
  word-wrap: break-word;
  /* Internet Explorer 5.5+ */
}

